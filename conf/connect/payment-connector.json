{
  "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
  "database.hostname": "postgres_payment",
  "database.port": "5432",
  "database.dbname": "auction",
  "database.user": "testUser",
  "database.password": "testPassword",
  "table.include.list": "public.payment_accounts,public.balance_operations",
  "plugin.name": "pgoutput",
  "publication.autocreate.mode": "filtered",
  "slot.name": "debezium",
  "publication.name": "dbz_publication",
  "tasks.max": 1,
  "topic.prefix": "auction",
  "tombstones.on.delete": false,
  "transforms": "unwrap,key,route",
  "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
  "transforms.unwrap.delete.tombstone.handling.mode": "tombstone",
  "transforms.unwrap.add.headers": "table:__TypeId__,op",
  "transforms.unwrap.add.headers.prefix": "",
  "transforms.key.type": "org.apache.kafka.connect.transforms.ExtractField$Key",
  "transforms.key.field": "id",
  "transforms.route.type": "org.apache.kafka.connect.transforms.RegexRouter",
  "transforms.route.regex": "auction\\.public\\.(.*)",
  "transforms.route.replacement": "auction.cdc.$1",
  "topic.creation.default.replication.factor": 1,
  "topic.creation.default.partitions": 5,
  "topic.creation.default.cleanup.policy": "compact,delete",
  "topic.creation.default.retention.ms": 604800000,
  "topic.creation.default.compression.type": "lz4",
  "topic.creation.default.min.insync.replicas": 1,
  "topic.creation.default.segment.bytes": 1073741824,
  "key.converter": "org.apache.kafka.connect.json.JsonConverter",
  "key.converter.schemas.enable": false,
  "value.converter": "org.apache.kafka.connect.json.JsonConverter",
  "value.converter.schemas.enable": false,
  "exactly.once.support": "required"
}